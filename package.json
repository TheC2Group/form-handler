{
  "name": "form-handler",
  "version": "2.0.1",
  "description": "Validate, read, and reset form fields.",
  "main": "form-handler.js",
  "scripts": {
    "example": "browserify example/js/main.js -o example/js/bundle.js",
    "test": "mocha",
    "standalone": "browserify -t [exposify --expose [ --jquery jQuery --c2-event-handler eventHandler ] ] form-handler.js --outfile standalone/form-handler.js --standalone FormFields",
    "dist": "uglifyjs standalone/form-handler.js --output standalone/form-handler.min.js --comments",
    "build": "npm run standalone && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TheC2Group/form-handler.git"
  },
  "keywords": [
    "form fields",
    "form validation"
  ],
  "author": "The C2 Group",
  "license": "MIT",
  "dependencies": {
    "c2-event-handler": "^2.3.1",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "exposify": "^0.5.0",
    "nightmare": "^2.0.6"
  },
  "bugs": {
    "url": "https://github.com/TheC2Group/form-handler/issues"
  },
  "homepage": "https://github.com/TheC2Group/form-handler#readme",
  "directories": {
    "example": "example",
    "test": "test"
  }
}
